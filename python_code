  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "\n",
    "#  CALCULATING THE TOTALS MEDALS OBTAINED BY EACH OF THE COUNTRIES DURING THE ANALYSIS PERIOD\n",
    "\n",
    "countries_medals_2000_2020 = hots_countries_medals[\n",
    "    (hots_countries_medals[\"game_year\"].isin(_years)) &\n",
    "    (~hots_countries_medals['medal_type'].isna())\n",
    "].reset_index(drop=True)\n",
    "\n",
    "# GETTING THE TOTAL MEDALS FOR EACH OF THE HOST COUNTRIES WHEN THEY HOSTED THE OLYMPICS\n",
    "hosts_medals_per_year = (\n",
    "    countries_medals_2000_2020.groupby([\"game_year\", \"country_code\"])[[\"medal_type\"]]\n",
    "    .count()\n",
    "    .reset_index()\n",
    "    .rename(columns={\"medal_type\": \"total_medals\"})\n",
    ")\n",
    "\n",
    "\n",
    "#  TRANSFORMIG THE YEAR_HOST COLUMNS TO STR SO WE CAN MERGE WITH THE HOST_MEDALS_PER_YEAR DATASET\n",
    "\n",
    "data_gdp_olympics['year_host'] = data_gdp_olympics['year_host'].astype(str)\n",
    "\n",
    "\n",
    "# MERGING THE GDP AND POPULATION STATISTICS WITH THE MEDALS PER YEAR OF THE HOST COUNTRIES, SO WE HAVE THE DATA FOR THE NETWORK ANALYSIS\n",
    "\n",
    "_data_network = data_gdp_olympics.merge(right=hosts_medals_per_year, how='left', right_on=['country_code', 'game_year'], left_on=['code', 'year_host'],)\n",
    "\n",
    "data_network = _data_network[['country', 'total_medals', 'population', 'GDP']]\n",
    "\n",
    "data_network = data_network.rename(columns={\n",
    "    'country': 'Country',\n",
    "    'total_medals': 'Medals',\n",
    "    'population': 'Population',\n",
    "    'GDP': 'GDP'\n",
    "})\n",
    "\n",
    "\n",
    "\n",
    "# STARTING TO CONSTRUCT THE NETWORK.\n",
    "\n",
    "df = pd.DataFrame(data_network)\n",
    "\n",
    "# Normalize Data\n",
    "for col in ['Medals', 'Population', 'GDP']:\n",
    "    df[f'{col}_Normalized'] = (df[col] - df[col].min()) / (df[col].max() - df[col].min())\n",
    "\n",
    "\n",
    "\n",
    "# Calculate Dominance Score\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "weights = {'Medals': 0.8, 'Population': 0.1, 'GDP': 0.1}\n",
    "\n",
    "\n",
    "df['Dominance_Score'] = (\n",
    "    weights['Medals'] * df['Medals_Normalized'] +\n",
    "    weights['Population'] * df['Population_Normalized'] +\n",
    "    weights['GDP'] * df['GDP_Normalized']\n",
    ")\n",
    "\n",
    "# Dominance Matrix\n",
    "dominance_matrix = np.zeros((len(df), len(df)))\n",
    "for i in range(len(df)):\n",
    "    for j in range(len(df)):\n",
    "        if df.loc[i, 'Dominance_Score'] > df.loc[j, 'Dominance_Score']:\n",
    "            dominance_matrix[i, j] = 1\n",
    "\n",
    "\n",
    "\n",
    "# Build Network\n",
    "G = nx.DiGraph()\n",
    "\n",
    "for i, country in enumerate(df['Country']):\n",
    "    G.add_node(country, score=df.loc[i, 'Dominance_Score'])\n",
    "\n",
    "for i in range(len(dominance_matrix)):\n",
    "    for j in range(len(dominance_matrix)):\n",
    "        if dominance_matrix[i, j] == 1:\n",
    "            G.add_edge(df.loc[i, 'Country'], df.loc[j, 'Country'])\n",
    "\n",
    "\n",
    "\n",
    "# Plot Network\n",
    "pos = nx.spring_layout(G, seed=42)\n",
    "plt.figure(figsize=(10, 8))\n",
    "nx.draw(\n",
    "    G, pos, with_labels=True, node_size=[v['score']*1000 for v in dict(G.nodes(data=True)).values()],\n",
    "    node_color='skyblue', edge_color='gray', arrowsize=20, font_size=10\n",
    ")\n",
    "\n",
    "\n",
    "plt.title('Dominance Network Analysis')\n",
    "plt.show()\n"
   ]
  },
