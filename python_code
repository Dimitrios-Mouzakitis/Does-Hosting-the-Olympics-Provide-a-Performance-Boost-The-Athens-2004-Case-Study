    "# Group and count the medals by year and medal type\n",
    "medal_counts = greece_medals_2000_2020.groupby(['game_year', 'medal_type'])['game_year'].count().unstack(fill_value=0)\n",
    "\n",
    "# Calculate total medals per year\n",
    "medal_counts['Total'] = medal_counts.sum(axis=1)\n",
    "\n",
    "# Plotting the bar plot\n",
    "ax = medal_counts.drop(columns='Total').plot(kind='bar', stacked=False, figsize=(10, 6), width=0.8, alpha=0.8)\n",
    "\n",
    "# Adding the line series for total medals\n",
    "ax.plot(medal_counts.index, medal_counts['Total'], color='black', marker='o', linestyle='-', linewidth=2, label='Total Medals')\n",
    "\n",
    "# Customize the plot\n",
    "plt.title(\"Greek Medals by Year (2000-2020) with Total Medals\")\n",
