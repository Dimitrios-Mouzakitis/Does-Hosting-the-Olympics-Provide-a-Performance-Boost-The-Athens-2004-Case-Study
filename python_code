  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x700 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "medal_counts = greece_medals_2000_2020.groupby(['game_year', 'medal_type'])['game_year'].count().unstack(fill_value=0)\n",
    "\n",
    "# Calculate total medals per year\n",
    "medal_counts['Total'] = medal_counts.sum(axis=1)\n",
    "\n",
    "# Create a figure and a set of subplots\n",
    "fig, ax1 = plt.subplots(figsize=(12, 7))\n",
    "\n",
    "# Plot the bar chart for each medal type\n",
    "medal_types = ['GOLD', 'SILVER', 'BRONZE']\n",
    "colors = ['gold', 'silver', '#cd7f32']  # Bronze color hex\n",
    "\n",
    "# Plot each medal type as a separate bar\n",
    "for medal, color in zip(medal_types, colors):\n",
    "    ax1.bar(medal_counts.index, medal_counts[medal], label=medal, color=color, width=0.6)\n",
    "\n",
    "# Set labels and title for the bar chart\n",
    "ax1.set_xlabel('Game Year', fontsize=12)\n",
    "ax1.set_ylabel('Count of Medals', fontsize=12)\n",
    "ax1.set_title('Greek Medals by Year (2000-2020)', fontsize=14)\n",
    "ax1.set_xticks(medal_counts.index)\n",
    "ax1.set_xticklabels(medal_counts.index, rotation=45)\n",
    "\n",
    "# Create a second y-axis for the total medals\n",
    "ax2 = ax1.twinx()\n",
    "ax2.plot(medal_counts.index, medal_counts['Total'], color='black', marker='o', linestyle='-', linewidth=2, label='Total Medals')\n",
    "ax2.set_ylabel('Total Medals', fontsize=12)\n",
    "\n",
    "# Combine legends from both axes\n",
    "handles1, labels1 = ax1.get_legend_handles_labels()\n",
    "handles2, labels2 = ax2.get_legend_handles_labels()\n",
    "ax1.legend(handles=handles1 + handles2, labels=labels1 + labels2, title=\"Medal Type and Total\", loc='upper left')\n",
    "\n",
    "# Adjust layout for better spacing\n",
    "plt.tight_layout()\n",
    "\n",
    "# Show the plot\n",
    "plt.show()\n"
   ]
